{
  "version": 2,
  "name": "mi-proyecto-html",
  "public": true,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "pag*/*.csv",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.js)",
      "headers": {
        "Content-Type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "/pag1/(.*\\.csv)",
      "headers": {
        "Content-Type": "text/csv"
      },
      "continue": true
    },
    {
      "src": "/pag2/(.*\\.csv)",
      "headers": {
        "Content-Type": "text/csv"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
